let today = new Date();
let month = today.getMonth(); // getMonth() returns 0 for January, 1 for February, ..., 10 for November, 11 for December

let isNovemberToMarch = (month >= 10 || month <= 2);

if (isNovemberToMarch) {
    node.warn("It is between November and March.");
} else {
    node.warn("It is not between November and March.");
}